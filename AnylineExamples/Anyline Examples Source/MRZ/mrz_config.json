{
    "cameraConfig": {
        "captureResolution": "1080p",
        "zoomGesture": true
    },
    "flashConfig": {
        "mode": "manual",
        "alignment": "top_left",
    },
    "viewPluginConfig": {
        "pluginConfig": {
            "id": "id",
            "cancelOnResult": true,
            "mrzConfig": {
                "faceDetectionEnabled": true,
                "strictMode": false,
                "cropAndTransformID": false
            }
        },
        "cutoutConfig": {
            "maxWidthPercent": "90%",
            "alignment": "center",
            "ratioFromSize": {
                "width": 25,
                "height": 17
            },
            "cropPadding": {
                "x": 30,
                "y": 90
            },
            "outerColor": "000000",
            "outerAlpha": 0.3,
            "strokeWidth": 2,
            "strokeColor": "FFFFFF",
            "cornerRadius": 4,
            "feedbackStrokeColor": "0099FF"
        },
        "scanFeedbackConfig": {
            "style": "rect",
            "visualFeedbackRedrawTimeout": 100,
            "strokeWidth": 1,
            "strokeColor": "0099FF",
            "fillColor": "220099FF",
            "beepOnResult": true,
            "vibrateOnResult": true,
            "blinkAnimationOnResult": false
        }
    }
}

