{
    "captureResolution": "1080p",
    "cutout": {
        "style": "rect",
        "maxWidthPercent": "70%",
        "alignment": "top_half",
        "ratioFromSize": {
            "width": 62,
            "height": 9
        },
        "outerColor": "000000",
        "outerAlpha": 0.3,
        "strokeWidth": 1,
        "strokeColor": "FFFFFF",
        "cornerRadius": 2,
        "feedbackStrokeColor": "0099FF"
    },
    "flash": {
        "mode": "manual",
        "alignment": "TOP_LEFT"
    },
    "visualFeedback": {
        "animation": "traverse_multi",
        "animationDuration" : 250,
        "style": "contour_rect",
        "strokeWidth": 2,
        "strokeColor": "0099FF"
    },
    "beepOnResult": true,
    "vibrateOnResult": true,
    "blinkAnimationOnResult": true,
    "cancelOnResult": true,
    "reportingEnabled": true

}
