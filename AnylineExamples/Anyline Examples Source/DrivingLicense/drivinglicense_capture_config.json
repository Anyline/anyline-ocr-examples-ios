{
    "captureResolution":"1080p",

    "cutout": {
        "style": "rect",
        "maxWidthPercent": "100%",
        "maxHeightPercent": "100%",
        "alignment": "center",
        "ratioFromSize" : {
            "width": 125,
            "height": 85
        },
        "strokeWidth": 2,
        "cornerRadius": 4,
        "strokeColor": "FFFFFF",
        "outerColor": "000000",
        "outerAlpha": 0.3,
        "feedbackStrokeColor": "0099FF"
    },
    "flash": {
        "mode": "manual",
        "alignment": "top_left"
    },
    "beepOnResult": true,
    "vibrateOnResult": true,
    "blinkAnimationOnResult": true,
    "cancelOnResult": true,
    "visualFeedback": {
        "style": "CONTOUR_POINT",
        "strokeColor": "0099FF",
        "strokeWidth": 2
    }
}
