            "cropOffset": { "x": 0, "y": -15 },
            "cornerRadius": 6,
            "strokeColor": "FFFFFF",
            "strokeWidth": 2,
            "outerColor": "000000",
            "feedbackStrokeColor": "0099FF",
            "outerAlpha": 0.3
        },
        "scanFeedbackConfig": {
            "style": "contour_rect",
            "animation": "traverse_multi",
            "animationDuration" : 250,
            "strokeWidth": 2,
            "cornerRadius": 2,
            "strokeColor": "0099FF",
            "fillColor": "330099FF",
            "beepOnResult": true,
            "vibrateOnResult": true,
            "blinkAnimationOnResult": true
        }
