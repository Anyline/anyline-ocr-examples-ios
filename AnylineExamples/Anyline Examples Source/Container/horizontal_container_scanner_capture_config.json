{
    "cameraConfig": {
        "captureResolution": "1080p",
        "pictureResolution": "1080p",
        "zoomGesture": true
    },
    "flashConfig": {
        "mode": "manual",
        "alignment": "top_left"
    },
    "viewPluginConfig": {
        "pluginConfig": {
            "id": "container-horizontal",
            "containerConfig": {
                "scanMode": "HORIZONTAL"
            },
            "cancelOnResult": true
        },
        "cutoutConfig": {
            "animation": "none",
            "maxWidthPercent": "80%",
            "width": 720,
            "alignment": "top_half",
