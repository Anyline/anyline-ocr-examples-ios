{
  "scanViewConfigDescription": "Optimized for demonstrating overlay + visual feedback feature for scanning of multiple barcodes simultaneously. All formats enabled.",
  "cameraConfig": {
    "captureResolution": "1080p",
    "zoomGesture": true,
    "maxZoomRatio": 20
  },
  "viewPluginConfig": {
    "pluginConfig": {
      "id": "barcode_overlay_visualfeedback",
      "barcodeConfig": {
        "fastProcessMode": true,
        "barcodeFormats": [
          "ALL"
        ],
        "parseAAMVA": true,
        "multiBarcode": true
      },
      "cancelOnResult": false
    },
    "cutoutConfig": {
      "maxWidthPercent": "90%",
      "maxHeightPercent": "85%",
      "ratioFromSize": {
        "width": 100,
        "height": 80
      },
      "alignment": "center",
      "strokeWidth": 3,
      "cornerRadius": 2,
      "strokeColor": "FFFFFF",
      "outerColor": "000000",
      "outerAlpha": 0.5,
      "feedbackStrokeColor": "0099FF"
    },
    "scanFeedbackConfig": {
      "style": "rect",
      "strokeWidth": 2,
      "strokeColor": "3C3C43",
      "fillColor": "223C3C43",
      "animationDuration": 150,
      "blinkAnimationOnResult": false,
      "beepOnResult": false,
      "vibrateOnResult": false
    },
    "uiFeedbackConfig": {
      "presets": [
        {
          "presetName": "simple_instruction_label",
          "presetAttributes": [
            {
              "attributeName": "instruction_text",
              "attributeValue": "Tap on a barcode to select it, tap again to unselect"
            }
          ]
        }
      ]
    }
  }
}
