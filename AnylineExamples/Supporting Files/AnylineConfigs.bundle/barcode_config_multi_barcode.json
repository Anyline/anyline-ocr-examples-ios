{
  "scanViewConfigDescription": "Optimized for full frame scanning of multiple barcodes simultaneously. All formats enabled.",
  "cameraConfig": {
    "captureResolution": "1080p"
  },
  "flashConfig": {
    "mode": "auto",
    "alignment": "bottom_right"
  },
  "viewPluginConfig": {
